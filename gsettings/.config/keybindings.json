[
  {
    "name": "Open Settings",
    "command": "gnome-control-center",
    "binding": "<Super>s"
  },
  {
    "name": "Open Kitty Terminal",
    "command": "/home/khalil/.local/bin/kitty",
    "binding": "<Super>Return"
  },
  {
    "name": "Open Kitty with tmux",
    "command": "/home/khalil/.local/bin/kitty -e tmux",
    "binding": "<Super>t"
  },
  {
    "name": "Open File manager",
    "command": "nautilus",
    "binding": "<Super>f"
  },
  {
    "name": "spicetify",
    "command": "/home/khalil/apps/spicetify-launcher",
    "binding": "<Super>x"
  },
  {
    "name": "Connect logitech speaker",
    "command": "/home/khalil/apps/bspeaker",
    "binding": "<Shift><Super>equal"
  },
  {
    "name": "Disconnect logitech speaker",
    "command": "/home/khalil/apps/disconnect_blth",
    "binding": "<Super>minus"
  },
  {
    "name": "Open Firefox",
    "command": "/home/khalil/.local/bin/firefox",
    "binding": "<Super>b"
  },
  {
    "name": "system-ressource-monitoring",
    "command": "/home/khalil/.local/bin/kitty -e btm",
    "binding": "<Ctrl><Alt>b"
  }
]
