[
  {
    "name": "Open Calculator",
    "command": "gnome-calculator",
    "binding": "<Super>c"
  },
  {
    "name": "Open Firefox",
    "command": "firefox",
    "binding": "<Super>f"
  },
  {
    "name": "Open Terminal",
    "command": "gnome-terminal",
    "binding": "<Alt>t"
  },
  {
    "name": "Close Window",
    "command": "wmctrl -c '$(xdotool getwindowfocus getwindowname)'",
    "binding": "<Ctrl>w"
  },
  {
    "name": "Take Screenshot",
    "command": "gnome-screenshot",
    "binding": "<Shift>Print"
  },
  {
    "name": "Mute Volume",
    "command": "amixer set Master toggle",
    "binding": "<Super>m"
  },
  {
    "name": "Lock Screen",
    "command": "gnome-screensaver-command --lock",
    "binding": "<Ctrl><Alt>l"
  },
  {
    "name": "Open File Manager",
    "command": "nautilus",
    "binding": "<Super>e"
  },
  {
    "name": "Show Applications Menu",
    "command": "gnome-shell --replace",
    "binding": "<Super>a"
  },
  {
    "name": "Switch to Workspace 1",
    "command": "wmctrl -s 0",
    "binding": "<Super>1"
  },
  {
    "name": "Switch to Workspace 2",
    "command": "wmctrl -s 1",
    "binding": "<Super>2"
  }
]
